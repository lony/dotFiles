[global_config]
  tab_position = bottom
[keybindings]
  copy = <Control>c
  paste = <Control>v
[profiles]
  [[default]]
    background_darkness = 0.8
    background_type = transparent
    cursor_color = "#aaaaaa"
    font = Monospace 12
    foreground_color = "#ffffff"
    show_titlebar = False
    use_custom_command = True
    custom_command = tmux new-session -A -s init
    use_system_font = False
[layouts]
  [[default]]
    [[[window0]]]
      type = Window
      parent = ""
    [[[child1]]]
      type = Terminal
      parent = window0
[plugins]
